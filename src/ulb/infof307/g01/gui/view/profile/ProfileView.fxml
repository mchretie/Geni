<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #ffffff;"
            xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ulb.infof307.g01.gui.view.profile.ProfileViewController">
   <children>
      <Label alignment="CENTER" layoutX="380.0" layoutY="155.0" text="Profile" textAlignment="CENTER" />
      <TextField fx:id="usernameField" layoutX="315.0" layoutY="214.0" promptText="User Name" />
      <PasswordField fx:id="passwordField" layoutX="315.0" layoutY="276.0" promptText="Password" />
      <Button fx:id="loginButton" layoutX="374.0" layoutY="335.0" mnemonicParsing="false" text="Login" onMouseClicked="#handleLoginClicked"/>
   </children>

</AnchorPane>
